        },
        "danish_keywords": {
          "type":       "keyword_marker",
          "keywords_path": "${fess.dictionary.path}da/protwords.txt"
        },
        "danish_stemmer": {
          "type":       "stemmer",
          "language":   "danish"
        },
        "spanish_stop": {
          "type":       "stop",

          "filter": [
            "truncate20_filter",
            "lowercase",
            "hungarian_stop",
            "hungarian_keywords",
            "hungarian_stemmer"
          ]
        },
        "indonesian_analyzer": {
          "type": "custom",
          "tokenizer":  "standard",
          "filter": [
            "truncate20_filter",
            "lowercase",

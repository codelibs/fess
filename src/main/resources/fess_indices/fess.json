{
  "settings": {
    "index": {
      "codec": "${fess.index.codec}",
      "refresh_interval": "1s",
      "number_of_shards": "${fess.index.number_of_shards}",
      "number_of_replicas": 0,
      "auto_expand_replicas": "${fess.index.auto_expand_replicas}"
    },
    "analysis": {
      "char_filter": {
        "mapping_fa_filter": {
          "type": "mapping",
          "mappings_path": "${fess.dictionary.path}fa/mapping.txt"
        },
        "mapping_ja_filter": {
          "type": "mapping",
          "mappings_path": "${fess.dictionary.path}ja/mapping.txt"
        },
        "mapping_filter": {
          "type": "mapping",
          "mappings_path": "${fess.dictionary.path}mapping.txt"
        },
        "traditional_chinese_convert": {
          "type": "fess_traditional_chinese_convert",
          "convert_type": "t2s"
        },
        "zero_width_spaces": {
            "type":       "mapping",
            "mappings": [ "\\u200C=> "]
        },
        "removeall_filter": {
          "type": "pattern_replace",
          "pattern":"(.*)",
          "replacement":""
        }
      },
      "filter": {
        "stemmer_en_filter": {
          "type": "stemmer",
          "name": "english"
        },
        "english_override": {
          "type":       "stemmer_override",
          "rules_path": "${fess.dictionary.path}en/stemmer_override.txt"
        },
        "possessive_stemmer_en_filter": {
          "type": "stemmer",
          "name": "possessive_english"
        },
        "stopword_en_filter": {
          "type": "stop",
          "stopwords_path": "${fess.dictionary.path}en/stopwords.txt"
        },
        "arabic_stop": {
          "type":       "stop",

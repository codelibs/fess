        "standard_analyzer": {
          "type": "custom",
          "char_filter": [
            "mapping_ja_filter"
          ],
          "tokenizer": "unigram_synonym_tokenizer",
          "filter": [
            "alphanum_word_filter",
            "cjk_bigram",
